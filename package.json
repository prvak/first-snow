{
  "name": "inuit",
  "version": "0.0.0",
  "description": "A game about Inuit",
  "main": "src/server/index.js",
  "scripts": {
    "start": "babel-node src/server/index.js",
    "build": "watchify src/client/js/main.js -t babelify --outfile src/client/js/bundle.js -v",
    "devel": "nodemon --config .nodemon.json src/server/index.js --exec babel-node",
    "browser": "browser-sync start --proxy localhost:3000 --files src/client/index.html src/client/js/bundle.js src/client/css/",
    "test": "npm run lint && npm run mocha",
    "mocha": "mocha --compilers js:babel-register --recursive",
    "lint": "eslint src/ test/",
    "coverage": "istanbul cover _mocha -- --compilers js:babel-register --recursive",
    "update": "ncu -u -a && npm update"
  },
  "author": "Prvaak",
  "license": "GPLv3",
  "dependencies": {
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "cookie-parser": "1.4.3",
    "eslint": "3.19.0",
    "eslint-plugin-react": "6.10.3",
    "express": "4.15.2",
    "express-dot-engine": "1.0.7",
    "express-flash": "0.0.2",
    "express-promise": "0.4.0",
    "express-session": "1.15.2",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "redux": "3.6.0",
    "sqlite": "2.5.0",
    "sqlite3": "3.1.8",
    "winston": "2.3.1",
    "ws": "2.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-register": "6.24.0",
    "babelify": "7.3.0",
    "browser-sync": "2.18.8",
    "browserify": "14.1.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "npm-check-updates": "2.10.4",
    "should": "11.2.1",
    "supertest": "3.0.0",
    "supertest-session": "3.0.0",
    "watchify": "^3.9.0"
  }
}
